package com.ex00.init.config;

import org.springframework.stereotype.Component;

import lombok.Data;

@Component
@Data
public class PersonCompoent {
	private String username;
	private int age;
	private String addr;
	
	PersonCompoent(){
		this.username = "티모";
		this.age = 33;
		this.addr = "탑";
	}
}
